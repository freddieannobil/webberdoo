{% extends 'CJ/layout/admin_base.html.twig' %}

{% block body %}
    <h1>Channel</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ channel.id }}</td>
            </tr>
            <tr>
                <th>Channelid</th>
                <td>{{ channel.channelId }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ channel.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ channel.description }}</td>
            </tr>
            <tr>
                <th>Publishedon</th>
                <td>{{ channel.publishedOn }}</td>
            </tr>
            <tr>
                <th>Defaultthumbnail</th>
                <td>{{ channel.defaultThumbnail }}</td>
            </tr>
            <tr>
                <th>Mediumthumbnail</th>
                <td>{{ channel.mediumThumbnail }}</td>
            </tr>
            <tr>
                <th>Highthumbnail</th>
                <td>{{ channel.highThumbnail }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_channel_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_channel_edit', { 'id': channel.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
