{% extends 'App/layout/admin_base.html.twig' %}

{% block body %}
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">{% block title %}{% endblock %}</h3>
                </div>
                <!-- /.box-header -->
                <!-- form start -->
                {% block form %}{% endblock %}
            </div>
            <!-- /.box -->
        </div>
        <div class="col-md-6">
            {% block links %}{% endblock %}
        </div>
    </div>
</section>
{% endblock %}
